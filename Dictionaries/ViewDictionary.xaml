<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:AutoPartsApp.ViewModels"
                    xmlns:views="clr-namespace:AutoPartsApp.Views.Pages">
    <DataTemplate DataType="{x:Type viewmodels:LoginViewModel}">
        <views:LoginView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:RegisterViewModel}">
        <views:RegisterView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AccountViewModel}">
        <views:AccountView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:CatalogViewModel}">
        <views:CatalogView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AnalogViewModel}">
        <views:AnalogView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ContactsViewModel}">
        <views:ContactsView />
    </DataTemplate>
</ResourceDictionary>